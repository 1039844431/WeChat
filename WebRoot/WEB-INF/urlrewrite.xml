<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"  "http://tuckey.org/res/dtds/urlrewrite3.1.dtd"> 

	<urlrewrite>
		<rule>
			<from>/index.html</from>　　
			<to>system/IndexAction_index.do</to>　　　　　　　　　
		</rule>		
		<rule>
			<from>/mobileIndex.html</from>　　
			<to>system/IndexAction_mobileIndex.do</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/issue.html</from>　　
			<to>system/IssueAppAction_home.do</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/mobileIssue.html</from>　　
			<to>system/IssueAppAction_mobileHome.do</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/userIndex.html</from>　　
			<to>system/UserInfoAction_userIndex.do</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/mobileLoginHome.html</from>　　
			<to>system/UserInfoAction_mobileLoginHome.do</to>　　　　　　　　　
		</rule>	
		<rule>
			<from>/gotoMobileCollect.html</from>　　
			<to>system/CollectAction_gotoMobileCollect.do</to>　　　　　　　　　
		</rule>	
		<rule>
			<from>/gotoMobileIssue.html</from>　　
			<to>system/CollectAction_gotoMobileIssue.do</to>　　　　　　　　　
		</rule>	
		<rule>
			<from>/myinfo.html</from>　　
			<to>system/UserInfoAction_setUserInfo.do</to>　　　　　　　　　
		</rule>		
		<rule>
			<from>/category_(.+?)_(.+?).html</from>　　
			<to>system/AppDetailAction_putCategory.do?CategoryKey=$1&amp;pageNO=$2</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/Mobile_(.+?)_(.+?).html</from>　　
			<to>system/AppDetailAction_putMobileCategory.do?CategoryKey=$1&amp;pageNO=$2</to>　　　　　　　　　
		</rule>	
		<rule>
			<from>/issueApp.html</from>　　
			<to>system/IssueAppAction_issue.do</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/mobileIssueApp.html</from>　　
			<to>system/IssueAppAction_mobileIssue.do</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/appDetail_(.+?).html</from>　　
			<to>system/AppDetailAction_detail.do?appID=$1</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/mobileAppDetail_(.+?).html</from>　　
			<to>system/AppDetailAction_mobileDetail.do?appID=$1</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/search.html&amp;key=(.+?)</from>　　
			<to>system/AppDetailAction_setSearchKey.do?searchKey=$1</to>　　　　　　　　　
		</rule>
		<rule>
			<from>/mobileSearch.html&amp;key=(.+?)</from>　　
			<to>system/AppDetailAction_setMobileSearchKey.do?searchKey=$1</to>　　　　　　　　　
		</rule>
	</urlrewrite>