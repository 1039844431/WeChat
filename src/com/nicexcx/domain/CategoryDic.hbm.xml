<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nicexcx.domain.CategoryDic" table="categoryDic">
		<id name="ID" type="string">
			<column name="ID" sql-type="varchar(32)" not-null="true"></column>
			<generator class="uuid"></generator>
		</id>		
		<property name="key" type="string">
			<column name="categoryDic_key" sql-type="varchar(4)"></column>
		</property>
		<property name="value" type="string">
			<column name="categoryDic_value" sql-type="varchar(20)"></column>
		</property>		

	</class>
</hibernate-mapping>